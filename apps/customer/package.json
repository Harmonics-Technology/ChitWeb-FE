    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.3",
    "eslint-config-sznm": "^2.0.3",
    "lint-staged": "^15.2.4",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.2.5",
    "standard-version": "^9.5.0",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@9.1.2"
